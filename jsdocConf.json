{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "opts": {
      "template": "node_modules/docdash",
      "destination": "./docs/",
      "recurse": true
    },
    "source": {
      "include": ["./src/InputController.js", "./src/FirstPersonCamera.js", "./src/FirstPersonCameraDemo.js", "./src/main.js", "README.md"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "docdash": {
      "sort": true,
      "search": true,
      "collapse": false
    }
  }